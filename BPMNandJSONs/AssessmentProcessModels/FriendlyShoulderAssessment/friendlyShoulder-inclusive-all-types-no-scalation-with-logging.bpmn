    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_068yvt8" name="Suggestion or compliment" sourceRef="GatewayType" targetRef="TaskAcknowledge" />
    <bpmn:startEvent id="RequestForm" name="Request Form">
